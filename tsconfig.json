{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "paths": {
      "#/*": ["app/*"]
    },
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}
